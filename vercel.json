{
  "version": 2,
  "root": "public",
  "headers": [
    {
      "source": "/(.*\\.tsx)",
      "headers": [
        { "key": "Content-Type", "value": "text/javascript" }
      ]
    },
    {
      "source": "/(.*\\.ts)",
      "headers": [
        { "key": "Content-Type", "value": "text/javascript" }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/((?!.*\\..*).*)",
      "destination": "/index.html"
    }
  ]
}